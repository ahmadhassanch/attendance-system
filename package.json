  "scripts": {
    "prisma:test:push": "yarn dotenv -e .env.test yarn prisma db push",
    "db:test:rm": "docker compose rm test-db -s -f -v && rm -rf ./database",
    "db:test:up": "docker compose up test-db -d",
    "db:test:restart": "yarn db:test:rm && yarn db:test:up && sleep 5",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:gen": "prettier --loglevel silent --print-width 80 --write \"src/modules/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "config:start:dev": "./scripts/configip.sh && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.test -- yarn jest --config ./jest-e2e.json",
    "test:e2e:cov": "dotenv -e .env.test -- yarn jest --config ./jest-e2e.json --coverage",
    "env:config": "./scripts/env-config.sh",
    "env:reset": "./scripts/env-reset.sh",
    "data:insert": "npx ts-node prisma/seed-data/main-fake.insert.ts",
    "migrate:reset": "yarn prisma migrate reset --force",
    "build:init": "./scripts/init.sh",
    "admissiondata:reset": "./scripts/data-admissiondata-reset.sh",
    "data:reset": "./scripts/data-reset.sh",
    "rebuild": "./scripts/rebuild.sh",
    "open": "open -a \"Google Chrome\" http://localhost:3000/rms/v1/swagger",
    "gen:dart": "./scripts/gen-dart.sh",
    "run:workflows": "./scripts/workflows.sh",
    "run:escalation:workflow": "yarn data:reset && ts-node RESOURCES/CHARMS/app-workflows/escalation-workflow/main-escalation-workflow.ts",
    "run:escalation:workflow:small": "yarn data:reset && ts-node RESOURCES/CHARMS/app-workflows/escalation-workflow/main-escalation-workflow-small.ts"
  },
